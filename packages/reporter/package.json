{
    "name": "@nirikshak/reporter",
    "version": "0.0.0",
    "description": "The jest reporter to analyse the logs generated by nirikshak tests",
    "keywords": [
        "QA",
        "nirikshak",
        "jest",
        "testing",
        "REST",
        "API"
    ],
    "author": "Yash Mahalwal <yashmahalwal@gmail.com>",
    "homepage": "https://github.com/yashmahalwal/nirikshak/tree/master/packages/reporter#readme",
    "license": "MIT",
    "main": "lib/index.js",
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yashmahalwal/nirikshak.git"
    },
    "scripts": {
        "build": "rm -rf lib && tsc -p tsconfig.build.json",
        "build:dev": "rm -rf lib && tsc -p tsconfig.watch.json",
        "test": "cd ../.. && npm run test:reporter",
        "test:update": "cd ../.. && npm run test:reporter -- -u",
        "watch": "rm -rf lib && tsc --watch -p tsconfig.watch.json",
        "lint": "cd ../.. && npm run lint:reporter",
        "format": "cd ../.. && npm run format:reporter",
        "commit": "cd ../.. && npm run commit"
    },
    "bugs": {
        "url": "https://github.com/yashmahalwal/nirikshak/issues"
    },
    "devDependencies": {
        "@jest/reporters": "^26.0.1",
        "@jest/test-result": "^26.0.1",
        "@types/fs-extra": "^9.0.1",
        "@types/lodash": "^4.14.155",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@nirikshak/core": "^0.0.0",
        "fs-extra": "^9.0.1",
        "lodash": "^4.17.15",
        "open": "^7.0.4",
        "signale": "^1.4.0"
    }
}
